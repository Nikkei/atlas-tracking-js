{
  "name": "atlas-tracking-js",
  "version": "2.16.3",
  "description": "ATJ: Atlas Tracking JS provides capabilities for measuring user activities on your website.",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:build && mocha-chrome ./test/test.html --no-colors --ignore-exceptions --chrome-flags '[\"--no-sandbox\"]' --chrome-launcher.maxConnectionRetries=100; npm run test:clean",
    "test:build": "webpack -m -c ./webpack.common.js -c ./webpack.dev.js",
    "test:clean": "rimraf test/build && mkdirp test/build",
    "build:npm": "rollup -c",
    "build:dist": "webpack -m -c ./webpack.common.js -c ./webpack.prd.js",
    "prepublishOnly": "npm run build:npm",
    "eslint": "eslint ./src --ext .js",
    "esdoc": "esdoc"
  },
  "author": "Nikkei Inc.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "chai": "~4.3.6",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^8.22.0",
    "gulp": "^4.0.2",
    "gulp-buffer": "0.0.2",
    "gulp-tap": "^2.0.0",
    "mkdirp": "^1.0.4",
    "mocha": "^10.0.0",
    "mocha-chrome": "^2.2.0",
    "query-string": "^7.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "uglify-js": "^3.17.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nikkei/atlas-tracking-js.git"
  },
  "bugs": {
    "url": "https://github.com/Nikkei/atlas-tracking-js/issues"
  },
  "homepage": "https://github.com/Nikkei/atlas-tracking-js#readme",
  "directories": {
    "test": "test"
  }
}
